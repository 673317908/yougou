/**index.wxss**/
swiper-item image{
  width:750rpx; 
}
.navbar{
  height: 193rpx;
  width: 750rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.navbar image{
  width: 128rpx;
  height: 140rpx;
  padding: 0 30rpx;
}
.floor_title{
  width: 750rpx;
  height: 58rpx;
  padding-top: 30rpx;
  background: #f4f4f4;
}
.floor_title image{
  width: 100%;
  height: 100%;
}
.product_list{
  overflow: hidden;
  padding: 20rpx 15rpx;
}
.product{
  float: left;
  width: 233rpx;
  height: 188rpx;
  margin: 5rpx;
}
.product:nth-child(1){
  height: 386rpx;
}
.product image{
  width: 100%;
  height: 100%;
}
.toTop{
  width: 100rpx;
  height: 100rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: center;
  background: rgba(0, 0, 0, .3);
  border-radius: 50%;
  position: fixed;
  right: 30rpx;
  bottom: 50rpx;
  color: white;
  font-size: 35rpx;
}